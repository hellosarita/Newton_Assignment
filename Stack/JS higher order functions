
function calculate(operation) {
    //  write your code here 
    if (operation === "ADD") {
    return function(value1, value2) {
      console.log(value1 + value2);
    };
  } else if (operation === "SUBTRACT") {
    return function(value1, value2) {
      console.log(value1 - value2);
    };
  }
     }
